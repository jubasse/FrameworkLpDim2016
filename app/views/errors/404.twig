{% extends 'layout.twig' %}

{% block title %}Page Not Found{% endblock %}
{% set mode = 'dev' %}
{% block body %}
    <p>
        Url "{{ request.path }}" Could not be found.
    </p>
    {% if 'dev' == mode %}
        <h2>Exception</h2>
        <p>{{ exception.message }}</p>
        <pre>
            {{- exception.traceAsString -}}
        </pre>
    {% endif %}
{% endblock %}